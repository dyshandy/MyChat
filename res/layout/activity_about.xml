<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.xiangde.dy.imchat.ui.AboutActivity" >

    <com.xiangde.dy.imchat.ui.customview.TemplateTitle
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:canBack="true"
        app:titleText="@string/about_title" />

    <com.xiangde.dy.imchat.ui.customview.LineControllerView
        android:id="@+id/imsdk"
        app:name="@string/about_imsdk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp" />

    <com.xiangde.dy.imchat.ui.customview.LineControllerView
        android:id="@+id/qalsdk"
        app:name="@string/about_qalsdk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.xiangde.dy.imchat.ui.customview.LineControllerView
        android:id="@+id/tlssdk"
        app:name="@string/about_tlssdk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.xiangde.dy.imchat.ui.customview.LineControllerView
        android:id="@+id/logLevel"
        app:name="@string/about_log"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:canNav="true"
        app:isBottom="true" />

</LinearLayout>